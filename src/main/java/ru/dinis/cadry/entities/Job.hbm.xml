<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.dinis.cadry.entities.Job" table="job" schema="cadry">
        <id name="id" column="id"/>
        <property name="userId" column="user_id"/>
        <property name="position" column="position"/>
        <property name="kategory" column="kategory"/>
        <property name="subdivision" column="subdivision"/>
        <property name="rate" column="rate"/>
        <property name="salary" column="salary"/>
        <property name="orderNumber" column="order_number"/>
        <property name="orderDate" column="order_date"/>
        <property name="emplDate" column="empl_date"/>
        <property name="orderDismissal" column="order_dismissal"/>
        <property name="orderDisDate" column="order_dis_date"/>
        <property name="disDate" column="dis_date"/>
        <property name="term" column="term"/>
        <property name="workHours" column="work_hours"/>
    </class>
</hibernate-mapping>